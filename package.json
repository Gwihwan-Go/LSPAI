{
  "name": "lspAi",
  "displayName": "lspAi",
  "publisher": "GwihwanGo",
  "description": "unit test generation with llm and lsp.",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/GwihwanGo/LSPAI.git"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lspAi.JavaExperiment",
        "title": "LSPAI::Java-Experiment"
      },
      {
        "command": "lspAi.GoExperiment",
        "title": "LSPAI::Go-Experiment"
      },
      {
        "command": "lspAi.PythonExperiment",
        "title": "LSPAI::Python-Experiment"
      },
      {
        "command": "extension.generateUnitTest",
        "title": "LSPAI::Generate Unit Test"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.generateUnitTest",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "LLM LSP Unit Test Generator",
      "properties": {
        "lspAi.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "API key for OpenAI"
        },
        "lspAi.deepseekApiKey": {
          "type": "string",
          "default": "",
          "description": "API key for Deepseek"
        },
        "lspAi.proxyUrl": {
          "type": "string",
          "default": "",
          "description": "Proxy URL for API calls"
        },
        "lspAi.localLLMUrl": {
          "type": "string",
          "default": "",
          "description": "Proxy URL for API calls"
        },
        "lspAi.model": {
          "type": "string",
          "default": "deepseek-chat",
          "description": "Model to use for code generation"
        },
        "lspAi.maxRound": {
          "type": "number",
          "default": 5,
          "description": "Maximum number of rounds for code generation"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "build": "tsc",
    "test": "node ./out/test/runTest.js",
    "experiment": "npm run build && NODE_PATH=./node_modules mocha ./out/runExp.js"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "mocha": "^11.0.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@langchain/core": "^0.3.26",
    "@langchain/openai": "^0.3.16",
    "https-proxy-agent": "^7.0.5",
    "lru-cache": "^10.1.0",
    "ollama": "^0.5.12",
    "openai": "^4.77.0"
  }
}
